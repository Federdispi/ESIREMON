package object;

import java.io.IOException;

import javax.imageio.ImageIO;

public class Bookshelf extends Object {
	public Bookshelf() {
		name = "Bookshelf";
		width = 48;
		height = 65;
		
		try {
			image = ImageIO.read(getClass().getResourceAsStream("/objects/armadio.png"));
		} catch(IOException e) {
			e.printStackTrace();
		}
	}
}
