package object;

import java.io.IOException;

import javax.imageio.ImageIO;

public class Table extends Object {
	
	public Table() {
		name = "Table";
		width = 32;
		height = 33;
		try {
			image = ImageIO.read(getClass().getResourceAsStream("/object/tavolo.png"));
		} catch(IOException e) {
			e.printStackTrace();
		}
	}

}
