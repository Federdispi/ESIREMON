package object;

import java.io.IOException;

import javax.imageio.ImageIO;

public class HorizontalDesk extends Object{
	
	public HorizontalDesk() {
		name = "HorizontalDesk";
		collision = true;
		width = 87;
		height = 59;
		try {
			image = ImageIO.read(getClass().getResourceAsStream("/objects/scrivania orizzontale.png"));
		} catch(IOException e) {
			e.printStackTrace();
		}
	}

}
