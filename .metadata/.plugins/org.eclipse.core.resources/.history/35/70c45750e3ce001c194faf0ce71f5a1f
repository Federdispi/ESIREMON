package application;

import object.Tree;

public class AssetManager {
	GamePanel gamePanel;
	public AssetManager(GamePanel gamePanel) {
		this.gamePanel = gamePanel;
	}
	
	public void setObject() {
		gamePanel.object[0] = new Tree();
		gamePanel.object[0].setX(1 * gamePanel.getTileSize());
		gamePanel.object[0].setY(2 * gamePanel.getTileSize());
		
		gamePanel.object[1] = new Tree();
		gamePanel.object[1].setX(5 * gamePanel.getTileSize());
		gamePanel.object[1].setY(2 * gamePanel.getTileSize());
	}
}
