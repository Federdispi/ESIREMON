package object;

import java.io.IOException;

import javax.imageio.ImageIO;

import application.GamePanel;

public class Coffee extends Object {
	
	public Coffee(GamePanel gamePanel) {
		super(gamePanel);
		name = "Café";
		price = 1.00F;
		description = "Café fait avec une nouvelle machine qui te fais récupérer 50PV";
		try {
			image = ImageIO.read(getClass().getResourceAsStream("/objects/caffe.png"));
		} catch(IOException e) {
			e.printStackTrace();
		}
	}

}
