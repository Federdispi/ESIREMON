package object;

import java.io.IOException;

import javax.imageio.ImageIO;

public class Bed extends Object {
	public Bed(GamePanel gamePanel) {
		super(gamePanel);
		name = "Bed";
		width = 48;
		height = 65;
		collision = false;	
		try {
			image = ImageIO.read(getClass().getResourceAsStream("/objects/letto.png"));
		} catch(IOException e) {
			e.printStackTrace();
		}
	}
}
